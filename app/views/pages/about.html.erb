          ['11/8',  51.1,      51.1,         51.2,             50.6],
          ['11/9',  52.5,      51.1,         51.6,             50.9],
          ['11/10',  52.5,      51.9,         51.6,            52.6],
          ['11/11',  53.3,      52.2,         51.6,             53.6],
          ['11/12',  53.2,      52.2,         54.6,             54.6],
          ['11/13',  52.6,      52.1,         55.6,             55.6],
          ['11/14',  52.6,      52.8,         58.6,             57.6],
          ['11/15',  53.6,      53.5,         59.6,             58.6],
        ]);

        var options = {
          colors: ['#3498db'],
          title : 'Compliance Rates: %',
          titleTextStyle: {color: '#3498db'},
          vAxis: {
            textPosition: 'in'
          },
          legend: {
            position: "top",
            textStyle: {color: '#3498db'},
            alignment: "end"
          },
          hAxis: {
          baselineColor: "#3498db",
          gridlinesColor: "grey",
          gridlinesCount: "-1",
          title: "Previous 14 days",
        },
          seriesType: "bars",
          series: {3: {type: "line"}},
          backgroundColor: 'white',
          chartArea: {
            width: "100%",
            height: "70%",
            backgroundColor: {
              stroke: '#3498db',
              strokeWidth: 1
            }
          }
        };

        var chart = new google.visualization.ComboChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
      google.setOnLoadCallback(drawVisualization);
    </script>
